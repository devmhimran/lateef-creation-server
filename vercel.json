{
    "version": 2,
    "builds":[
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/(.*)",
            "dest": "/"
        },
        {
            "src": "/data",
            "method": [
                "GET"
            ],
            "dest": "/index.js"
        }
    ]
}